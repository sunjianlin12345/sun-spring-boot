# 数据库驱动
jdbc.driver=com.mysql.jdbc.Driver
# 数据库url
jdbc.url=jdbc:mysql://localhost:3306/test
# 用户名
jdbc.username=mysql
# 密码
jdbc.password=mysql

#注释
author=sun

# 数据库表名 table_cc
tableName=table_name

# 配置路径
targetRoot=\\src\\main\\java
targetResources=\\src\\main\\resources

targetGroup=\\com\\sun\\sunspringboot
moduleName=com.sun.sunspringboot

entityTargetPackage=entity
daoTargetPackage=dao
serviceTargetPackage=service
mapperTargetPackage=mapper

